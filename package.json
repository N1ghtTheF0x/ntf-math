  "author": "N1ghtTheF0x",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/N1ghtTheF0x/ntf-math/issues"
  },
  "homepage": "https://github.com/N1ghtTheF0x/ntf-math#readme",
  "devDependencies": {
    "tsdown": "latest",
    "typescript": "latest"

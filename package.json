{
  "name": "@ntf/math",
  "version": "1.3.2",
  "description": "Another math library",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "require": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "scripts": {
    "pretest": "pnpm build",
    "test": "tsx --tsconfig ./tests/nodejs/tsconfig.json ./tests/nodejs/test.cts",
    "check": "tsc --noEmit",
    "prebuild": "pnpm check",
    "build": "pnpm build:prod --sourcemap",
    "build:prod": "tsup",
    "prepublishOnly": "npm build:prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/N1ghtTheF0x/ntf-math.git"
  },
  "keywords": [
    "esm",
    "cjs",
    "nodejs",
    "browser",
    "math",
    "vector2",
    "vector3",
    "matrix"
  ],
  "author": "N1ghtTheF0x",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/N1ghtTheF0x/ntf-math/issues"
  },
  "homepage": "https://github.com/N1ghtTheF0x/ntf-math#readme",
  "devDependencies": {
    "@types/node": "latest",
    "express": "latest",
    "tsup": "latest",
    "tsx": "latest",
    "typescript": "latest"
  }
}

{
  "name": "@ntf/math",
  "version": "1.3.0",
  "description": "Another math library",
  "files": ["dist"],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "scripts": {
    "test": "tsx --project tsconfig.test.json test.ts",
    "build": "pkgroll --src source",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/N1ghtTheF0x/ntf-math.git"
  },
  "keywords": [
    "esm", "cjs", "nodejs", "browser", "math", "vector2", "vector3", "matrix"
  ],
  "author": "N1ghtTheF0x",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/N1ghtTheF0x/ntf-math/issues"
  },
  "homepage": "https://github.com/N1ghtTheF0x/ntf-math#readme",
  "devDependencies": {
    "typescript": "latest",
    "tsx": "latest",
    "express": "latest",
    "pkgroll": "latest"
  }
}
